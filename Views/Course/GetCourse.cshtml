@{
    @model IEnumerable<Student>

}


@{
    if (Model.Count() <= 0)
    {
        <h2 class="alert alert-warning">There is no Student studying this course please verify your infos </h2>
    }
    else

    {
        <h1>@ViewBag.Id</h1>
        <div >

            @foreach(Student s in Model){
            <div >
                @s.first_name
            </div>

            }

        </div>
    }


}


<a asp-controller="Course" asp-action="index"> go back</a>